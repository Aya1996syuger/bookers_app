<h1>Editing Book</h1>
<%= form_with model:@book, url:booke_path(@book), local:true do |f| %>

  <h4>タイトル</h4>
  <%= f.text_field :title %>

  <h4>本文</h4>
  <%= f.text_area :body %>

  <%= f.submit 'Update Book' %>

<% end %>

<%= link_to "edit", edit_book_path(@book) %>
<%= link_to "back", books_path %>